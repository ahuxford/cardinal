[GENERAL]
polynomialOrder = 7
numSteps = 5
dt = 2e-02
timeStepper = tombo2
writeInterval = -1

[PRESSURE]
residualTol = 1e-06

[VELOCITY]
boundaryTypeMap = v, O, W
residualTol = 1e-08
density = 1.0     # added for Reynolds number
viscosity = 0.001 # added for Reynolds number

[TEMPERATURE]
boundaryTypeMap = t, O, I
residualTol = 1e-08
rhoCp = 1.0          # added for Peclet Number
conductivity = 0.001 # added for Peclet Number
