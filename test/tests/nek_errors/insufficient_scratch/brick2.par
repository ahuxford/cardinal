[OCCA]
  backend = CPU

[GENERAL]
  stopAt = numSteps
  numSteps = 1000
  dt = 5.0e-4
  polynomialOrder = 2
  writeControl = steps
  writeInterval = 2
  oudf = "brick.oudf"
  udf = "brick.udf"

[PROBLEMTYPE]
  stressFormulation = true

[MESH]
#  boundaryTypeMap = symy,symy,inlet,outlet,symz,symz
  solver = elasticity
  file = "brick.re2"

[VELOCITY]
  viscosity = 1.0
  density = 1.0
  residualTol = 1.0e-6
  boundaryTypeMap = symy, symy, fixedValue+moving, outlet, symz, symz

[PRESSURE]
  residualTol = 1.0e-5

[TEMPERATURE]
  residualTol = 1e-6
  boundaryTypeMap = t, f, f, f, f, f
